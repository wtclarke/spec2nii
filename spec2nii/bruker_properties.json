{
    "SpectrometerFrequency": [
      {
        "cmd": "[#VisuAcqImagingFrequency,]",
        "conditions": ["@type=='2dseq'"]
      },
      {
        "cmd": "[#BF1,]",
        "conditions": ["@type in ['fid', 'fid_proc', 'rawdata']"]
      }
    ],
    "ResonantNucleus": [
      {
        "cmd": "[#VisuAcqImagedNucleus[1:-1]]",
        "conditions": ["@type=='2dseq'"]
      },
      {
        "cmd": "[#NUCLEUS[1:-1]]",
        "conditions": ["@type in ['fid', 'fid_proc', 'rawdata']"]
      }
    ],
    "is_spectroscopy": [
      {
        "cmd": "'spectroscopic' in #VisuCoreDimDesc.list",
        "conditions": ["@type=='2dseq'"]
      },
      {
        "cmd": "'Spectroscopic' in #ACQ_dim_desc.list",
        "conditions": ["@type in ['fid', 'fid_proc', 'rawdata']"]
      }
    ],
    "is_complex": [
      {
        "cmd": "True",
        "conditions": [
          "'REAL_IMAGE' in #VisuCoreFrameType.list",
          "'IMAGINARY_IMAGE' in #VisuCoreFrameType.list"
        ]
      },
      {
        "cmd": "True",
        "conditions": ["@type in ['fid', 'fid_proc', 'rawdata']"]
      },
      {
        "cmd": "False",
        "conditions": [
        ]
      }
    ],
    "is_mrsi": [
      {
        "cmd": "True",
        "conditions": [
          "@type=='2dseq'",
          "#VisuCoreDimDesc.list==['spectroscopic', 'spatial', 'spatial']"
        ]
      },
      {
        "cmd": "True",
        "conditions": [
          "@type in ['fid', 'fid_proc', 'rawdata']",
          "#ACQ_dim_desc.list==['Spectroscopic', 'Spatial', 'Spatial']"
        ]
      },
      {
        "cmd": "False",
        "conditions": []
      }
    ],
    "is_svs": [
      {
        "cmd": "True",
        "conditions": [
          "@type=='2dseq'",
          "#VisuCoreDimDesc.list==['spectroscopic',]"
        ]
      },
      {
        "cmd": "True",
        "conditions": [
          "@type in ['fid', 'fid_proc', 'rawdata']",
          "#ACQ_dim_desc.list==['Spectroscopic',]"
        ]
      },
      {
        "cmd": "False",
        "conditions": []
      }
    ],
    "points_prior_to_echo": [
      {
        "cmd": "#PVM_DigShift",
        "conditions": ["@type in ['fid', 'fid_proc', 'rawdata']"]
      },
      {
        "cmd": "int(np.round(#ACQ_RxFilterInfo[0][0]))",
        "conditions": ["@type in ['fid', 'fid_proc', 'rawdata']",
                      "#ACQ_sw_version[0:8]=='<PV-360.'"
        ]
      },
      {
        "cmd": "0",
        "conditions": []
      }
    ],
    "method_desc": [
      {
        "cmd": "#Method",
        "conditions": ["@type in ['fid', 'fid_proc', 'rawdata']"]
      },
      {
        "cmd": "#VisuAcquisitionProtocol",
        "conditions": ["@type=='2dseq'"]
      }
    ],
    "PV_version": [
      {
        "cmd": "#ACQ_sw_version",
        "conditions": ["@type in ['fid', 'fid_proc', 'rawdata']"]
      },
      {
        "cmd": "#VisuAcqSoftwareVersion",
        "conditions": ["@type=='2dseq'"]
      }
    ],
    "working_offset": [
      {
        "cmd": "#PVM_FrqWorkOffsetPpm",
        "conditions": []
      }
    ],
    "freq_ref": [
      {
        "cmd": "#PVM_FrqRef",
        "conditions": []
      }
    ],
    "dwell_time": [
      {
        "cmd": "1./ #PVM_SpecSWH",
        "conditions": []
      }
    ],
    "method_TE": [
      {
        "cmd": "#PVM_EchoTime",
        "conditions": ["@type=='2dseq'"],
        "unit": "ms"
      }
    ],
    "method_TR": [
      {
          "cmd": "#PVM_RepetitionTime",
          "conditions": ["@type=='2dseq'"],
          "unit": "ms"
      }
    ]
  }
